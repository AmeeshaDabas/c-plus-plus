#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int n,i;
	    cin>>n;
	    int arr[n];
	    for(i=0;i<n;i++)
	    {
	        cin>>arr[i];
	    }
	    sort(arr,arr+n);
	    long long int sum=0;
	    for(i=0;i<=(n-i-1);i++)
	    {
	        sum+=(arr[i]*arr[n-i-1]);
	    }
	    cout<<sum<<endl;
	}
	return 0;
}
