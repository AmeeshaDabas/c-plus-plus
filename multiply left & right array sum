#include <iostream>
using namespace std;

int main() {
	int n,t,i;
	cin>>t;
	while(t--)
	{
	    cin>>n;
	    int arr[n],sum1=0,sum2=0;
	    for(i=0;i<n;i++)
	    {
	        cin>>arr[i];
	    }
	    for(i=0;i<n/2;i++)
	    {
	        sum1+=arr[i];
	    }
	    for(i=n/2;i<n;i++)
	    {
	        sum2+=arr[i];
	    }
        cout<<(sum1*sum2)<<endl;
	}
	return 0;
}
