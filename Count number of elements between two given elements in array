#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int n,i;
	    cin>>n;
	    int arr[n];
	    for(i=0;i<n;i++)
	    {cin>>arr[i];}
	    int num1,num2,n1,n2;
	    cin>>num1>>num2;
	    for(i=0;i<n;i++)
	    {
	     if(arr[i]==num1)
	    { n1=i;
	     break;
	    }
	    }
	    for(i=(n-1);i>=0;i--)
	    {
	     if(arr[i]==num2)
	    { n2=i;
	     break;
	    }
	    }
	    cout<<(n2-n1-1)<<endl;
	}
	return 0;
}
