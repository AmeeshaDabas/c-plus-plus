#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int n,c=0;
	    cin>>n;
	    int i,arr[n];
	    for(i=0;i<n;i++)
	    {
	        cin>>arr[i];
	    }
	    sort(arr,arr+n);
	    for(i=0;i<n;i++)
	    {
	      if(arr[i]==arr[i+1]) 
	      c++;
	    }
	    if(!c)
	    cout<<"GIRLS\n";
	    else
	    cout<<"BOYS\n";
	   
	}
	return 0;
}
