#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int n,i;
	    cin>>n;
	    int arr[n];
	    for(i=0;i<n;i++)
	    {
	        cin>>arr[i];
	    }
	    sort(arr,arr+n);
	    for(i=0;i<=(n-i-1);i++)
	    {
	       if(i==(n-i-1))
	       {
	           cout<<arr[i]<<" ";
	       }
	       else
	       cout<<arr[i]<<" "<<arr[n-i-1]<<" ";
	    }
	    cout<<"\n";
	}
	return 0;
}
